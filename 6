programa
{
	funcao inicio()
	{
		inteiro semanas_de_seis_dias = 365/6
		inteiro minutos_ano = semanas_de_seis_dias*30 // Em 6 dias, são dedicados 30 minutos para o livro. Por isso usei "30".
		real horas = minutos_ano/60
		
		escreva("A quantidade de horas no ano dedicadas ao livro será ", horas, ".")		
	}
}
