programa
{
	
	funcao inicio()
	{
		inteiro ano

		escreva("*** IDENTIFICADOR DE BISSEXTO ***")

		escreva("Informe um ano: ")
		leia(ano)

		se(ano < 1) {
			ano = 2023
		}

		se(ano % 4 == 0){

			se(ano % 100 == 0 ) {

				se(ano % 400 == 0) {
					escreva("O ano ", ano, " é bissexto")
				}senao {
					escreva("O ano ", ano, " não é bissexto")
				}
				
			} senao {
				escreva("O ano ", ano, " é bissexto")
			}
			
		} senao {
			escreva("O ano ", ano, " não é bissexto")
		}
	}
}
