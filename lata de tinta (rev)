programa
{
	inclua biblioteca Matematica
	
	
	funcao inicio()
	{
		//latas de 18 litros
		//1 litro de tinta pinta 3 metros quadrados
		// cada lata pinta 54 metros

		inteiro cobertura_tinta = 3
		inteiro capacidade_lata = 18
		inteiro numero_latas = 0
		
		//TAMANHO DA AREA A SER PINTADA
		real area = 109

		
		real litros_real = area / 3

		inteiro litros_sem_sobra = litros_real

		numero_latas = litros_real / capacidade_lata
		
		se((litros_real / capacidade_lata) > numero_latas){
			numero_latas++
		}

		escreva(numero_latas)
		
	}
