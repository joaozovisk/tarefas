programa
{
	funcao inicio()
	{
		escreva("Há ", 365*24, " horas em um ano normal e ", 366*24, " em um ano bissexto.")
		escreva("\n", "Há ", 60*24*365*10, " minutos em uma década.")
		escreva("\n", "Minha idade em segundos é ", 15*365*24*60*60, ".")
		escreva("\n", "Pretendo comer uns chocolate ae.")
		escreva("\n", "Sua idade em anos é ", 977000000/365/24/60/60, ".")
	}
}
